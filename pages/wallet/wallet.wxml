<!--pages/wallet/wallet.wxml-->
<view class="wrapper">
  <view class="money">
    <text class="one">我的余额（元）</text>
    <text class="tow">{{num}}</text>
    <text class="three" bindtap="click" data-name="yue">余额说明</text>
  </view>
  <button bindtap="oMoney">充值</button>
  <view class="detil">
    <view bindtap="click" data-name="juan">优惠券<text class="j">{{juan}}张 <text>></text></text></view>
    <view bindtap="click" data-name="tui">我的押金<text class="yj"></text></view>
    <view bindtap="click" data-name="guan">帮助<text>></text></view>
  </view>
</view>

